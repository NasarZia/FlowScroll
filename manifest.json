
{
  "manifest_version": 3,
  "name": "FlowScroll",
  "version": "1.0",
  "description": "A simple tool that lets you auto-scroll through web pages at a custom speed.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [
    "activeTab",
    "scripting"
  ]
}
